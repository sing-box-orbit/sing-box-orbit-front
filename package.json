{
	"name": "sing-box-orbit-front",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc -b && vite build",
		"preview": "vite preview",
		"lint": "biome lint .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"typecheck": "vue-tsc --noEmit",
		"schema": "dotenv -- bash -c 'gql.tada generate schema $VITE_API_URL/graphql --output ./src/graphql/schema.graphql'",
		"codegen": "bun run schema && gql.tada generate output"
	},
	"dependencies": {
		"@element-plus/icons-vue": "^2.3.2",
		"@urql/vue": "^2.0.0",
		"@vueuse/core": "^14.1.0",
		"better-auth": "^1.4.10",
		"element-plus": "^2.13.0",
		"graphql": "^16.12.0",
		"typebox": "^1.0.76",
		"vue": "^3.5.26",
		"vue-i18n": "^11.2.8",
		"vue-router": "^4.6.4"
	},
	"devDependencies": {
		"@0no-co/graphqlsp": "^1.15.2",
		"@biomejs/biome": "^2.3.11",
		"@iconify/json": "^2.2.426",
		"@types/node": "^25.0.3",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/tsconfig": "^0.8.1",
		"dotenv-cli": "^11.0.0",
		"gql.tada": "^1.9.0",
		"typescript": "~5.9.3",
		"unplugin-icons": "^22.5.0",
		"vite": "npm:rolldown-vite@7.3.1",
		"vue-tsc": "^3.2.2"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@7.3.1"
	}
}
